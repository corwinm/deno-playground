  "version": "0.2.0",
  "configurations": [
    {
      "name": "hello world",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/src",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "hello-world.ts"
      ],
      "port": 9229
    },
    {
      "name": "http server",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/src",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--allow-net",
        "http-server.ts"
      ],
      "port": 9229
    },
    {
      "name": "test",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/src",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-brk"
      ],
      "port": 9229
    }
  ]
}